<!DOCTYPE html>
<html lang="sv-se">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script src="https://kit.fontawesome.com/6a0526b489.js" crossorigin="anonymous"></script>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <link rel="icon" href="/images/freakyfashion-icon-32x32.png" sizes="32x32">
    <link rel="stylesheet" href="/stylesheets/site.min.css">
    <title>Administration</title>
</head>

<body>

    <!-- header section -->
    <%- include("../../partials/admin-header") %>

    <!-- main section -->
    <main>

        <section id="products-admin">

            <%- include("../../partials/admin-sidebar") %>
            
            <div class="products-view-all">

                <div class="box1">
                    <div class="search-field">
                        <i id="search-icon" class="fa-solid fa-magnifying-glass"></i>
                        <input id="search-bar" type="text" placeholder="Sök produkt">
                        <button type="button" class="btn-search btn-theme">Sök</button>
                    </div>
                    <a href="/admin/products/new" class="btn-new btn-theme">Ny produkt</a>
                </div>
    
                <div class="products-view">
    
                    <div class="product-details">
                        <h6>Namn</h6>
                    </div>
                    <div class="product-details">
                        <h6>SKU</h6>
                    </div>
                    <div class="product-details">
                        <h6>Publiserad</h6>
                    </div>
                    <div class="product-details">
                        <h6>Pris</h6>
                    </div>

                </div>

            </div>

        </section>

    </main>

    <script src="/js/products-index.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>
</body>

</html>